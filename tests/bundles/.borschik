{
    "spriter" : {

        "./" : {
            "default": {
                "path": "./_sprited",
                "name": "common"
            }
        },


        "./a/" : {
            "default": {
                "name": "common_a"
            }
        },

        "./a/_a_add.css" : {
            "default": {
                "ifexist": "add"
            }
        },

        "./a/_a_use.css" : {
            "default": {
                "ifexist": {
                    "method": "use",
                    "new": {
                        "name": "common_a_use_a"
                    }
                }
            }
        },


        "./b/" : {
            "default": {
                "name": "common_a",
                "ifexist": {
                    "method": "use",
                    "new": {
                        "name": "common_b_use_a"
                    }
                }
            }
        },

        "./b/_b_add.css" : {
            "default": {
                "name": "common_a_use_a",
                "ifexist": {
                    "method": "use",
                    "new": {
                        "name": "common_b_use_a_use_a"
                    }
                }
            }
        },


        "./c/" : {
            "default": {
                "name": "common_c",
                "ifexist": "use"
            }
        },

        "./c/_c1.css" : {
            "default": {
                "name": "common_c_1",
                "ifexist": "use"
            }
        },

        "./c/_c2.css" : {
            "default": {
                "name": "common_c_2",
                "ifexist": "use"
            }
        }

    }
}
